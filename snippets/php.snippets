snippet st "declare strict type"
    declare(strict_types=1);

snippet pe "pretty array/json log"
    print_r(json_encode([${0}], JSON_PRETTY_PRINT));exit;

snippet __ "construct"
    public function __construct(${0}) {}

snippet func "new function"
    public function ${1:functionName}()
    {
        ${2://}
    }
snippet tryb "try catch block"
    try {
        ${1://}
    } catch (${2:\Throwable} ${3:\$e}) {
        ${4://}
    }
